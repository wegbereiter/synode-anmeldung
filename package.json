{
  "name": "angular2-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./node_modules ./coverage ./dist && npm cache clean",
    "clean:dist": "rimraf ./dist",
    "clean:coverage": "rimraf ./coverage",
    "start": "webpack-dev-server --inline --progress --port 8080 --config config/webpack.dev.js",
    "build": "npm run clean:dist && webpack --progress --profile --bail --config config/webpack.prod.js",
    "lint": "tslint --force src/**/*.ts",
    "test": "npm run clean:coverage && karma start",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "posttest": "npm run coverage",
    "coverage": "npm run coverage:remap && npm run coverage:report",
    "coverage:remap": "remap-istanbul -i coverage/coverage.json -o coverage/coverage.json -t json -e node_modules,karma.entry.ts",
    "coverage:report": "istanbul report"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
